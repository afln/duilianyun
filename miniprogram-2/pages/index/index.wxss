.container {
  height: 100%;
  display: flex;
  width: 100%;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  background: rgb(243, 245, 244);
}
.container .search{
  width: 736rpx;
  height: 70rpx;
  padding: 12.5rpx 0 12.5rpx 15rpx;
  background: rgb(229, 42, 42);
}
.container .search-left{
  flex: 45;
  background: rgb(231, 78, 78);
  text-align: left;
  display: flex;
  flex-direction: row;
}
.container .search-left input{
  display: inline-block;
  height: 65rpx;
  font-size: 29rpx;
}
.search-placeholder{
  color: #fff;
  line-height: 20rpx;
  margin-top:26rpx;
  font-size: 14px;
}
.container .search .search-left image{
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}

 .search-left input{
  width: 500rpx;
  margin-top: 3rpx;
