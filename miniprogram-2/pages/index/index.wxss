}

.view{
  border-radius: 8px;
  width: 100%;
}

.container .search .search-right{
  flex: 1;
}
.flexWrp{
  display: flex;
}
.flexTab{
  flex-flow: row nowrap;
  justify-content: space-around;
  align-items: stretch;
}
.flexItem{
  flex-grow: 1;
  text-align: center;
  border-radius: 4px;
}
.container{
  width: 100%;
  padding: 0;
  font-size: 14rpx;
  color: rgb(0, 0, 0);
}
.container .feedItem{
  width: 685rpx;
  padding: 30rpx 30rpx 20rpx;
  margin: 7rpx 0 6rpx 0;
  background: #ffffff;
  border-top: 1px solid #eee;
  border-bottom: 1px solid #eee;
  box-shadow: 0 2px 5px #eeeeee;
  border-radius: 8px;
}

.container .feedItem .feedContent .question .label text{
  font-size: 24rpx;
  font-weight: 600px;
  line-height: 40rpx;
  color: rgb(212, 19, 19);
}
.container .feedItem .feedSource{
  width: 690rpx;
  left: 0;
  height: 50rpx;
}
.container .feedItem .feedSource .avatar{
  position: relative;
  display: inline-block;
}
.container .feedItem .feedSource a{
  display: inline-block;
  height: 40rpx;
}
.container .feedItem .feedSource .avatar image{
  /*position: absolute;*/
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  border-radius: 50rpx;
  top: 10rpx;
  vertical-align: middle;
}
.container .feedItem .feedSource text{
  /*position: absolute;*/
  display: inline-block;
  height: 40rpx;
  line-height: 40rpx;
  vertical-align: middle;
  margin: 0 0 0 15rpx;
  color: #a0acac;
  font-size: 26rpx;
}
.container .feedItem .feedSource .itemMore{
  display: inline-block;
  width: 40rpx;
  height: 45rpx;
}
.container .feedItem .feedContent{
  padding: 10rpx 0 0 0;
}
.container .feedItem .feedContent .question text{
  font-size: 28rpx;
  font-weight: 600px;
  line-height: 40rpx;
}
.container .feedItem .feedContent .answerBody{
  padding: 10rpx 0 0 0;
  /*height: 10rpx;*/
  font-size: 24rpx;
  line-height: 28rpx;
  color: #5b5b5b;
}
.container .feedItem .feedContent .answerActions{
  width: 690rpx;
  padding: 10rpx 0  0;
  color: #a0acac;
}
.container .feedItem .feedContent .answerActions view{
  display: inline-block;
  vertical-align: text-bottom;
  padding: 0 10rpx 0 0;
  font-size: 24rpx;
}

.container .feedItem .feedContent .answerActions .dot ::after{
  content: "•";
}
.container .feedItem .feedContent .question .label text{
  font-size: 24rpx;
  font-weight: 600px;
  line-height: 40rpx;
  color: rgb(212, 19, 19);
}
.container .feedItem .feedContent .answerActions .postTime {
  margin-right:  -1%;
  float: right;
}

